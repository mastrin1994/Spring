<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="myLogger" class="wyznikiewicz.spring.implementations.LoggerImpl">
		<property name="name" value="myLogger"/>
		<property name="version" value="3"/>
	</bean>
	<bean id="usersRepository" class="wyznikiewicz.spring.implementations.UsersRepositoryImpl">
		<constructor-arg ref="myLogger"></constructor-arg>
		<constructor-arg index="1" value="http://127.0.0.1:8080"></constructor-arg>
		<constructor-arg index="2" value="db"></constructor-arg>
	</bean>
</beans>
